{
  "name": "next-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "origin-dev": "next dev",
    "dev": "node ./server/main.js",
    "build": "next build",
    "start": "next start",
    "deploy": "bash ./deploy/deploy.sh",
    "export": "npm run build && next export",
    "lint": "npm run lint-pages && npm run lint-components && npm run lint-store && npm run lint-server && npm run lint-lib",
    "lint-pages": "eslint pages/* --cache --fix",
    "lint-components": "eslint components/* --cache --fix",
    "lint-store": "eslint store/* --cache --fix",
    "lint-server": "eslint server/* --cache --fix",
    "lint-lib": "eslint lib/* --cache --fix",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "**/*.js": "eslint --ext .js",
    "**/*.jsx": "eslint --ext .jsx"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "koa": "^2.8.1",
    "koa-router": "^7.4.0",
    "next": "9.0.5",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "chalk": "^2.4.2",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.5",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.5",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.12.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  }
}
